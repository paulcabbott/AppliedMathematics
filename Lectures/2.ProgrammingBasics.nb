Cell[CellGroupData[{

Cell["\<\
Here's a couple of ways of coding the factorial function in a procedural \
manner:\
\>", "MathCaption",
 CellChangeTimes->{{3.447647328061961*^9, 3.447647356490652*^9}, 
   3.447652774136093*^9, {3.447900553030191*^9, 3.447900555722065*^9}, {
   3.479989965300515*^9, 3.4799899655065937`*^9}, {3.539928701282831*^9, 
   3.539928701563244*^9}},ExpressionUUID->"33b084f7-ac31-4e56-8099-\
f33be9ebe543"],

Cell[BoxData[
 RowBox[{
  RowBox[{"facProcedural1", "[", "n_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
      RowBox[{"s", "=", "1"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"i", "<", "n"}], ",", 
       RowBox[{
        RowBox[{"i", "++"}], ";", 
        RowBox[{"s", "=", 
         RowBox[{"i", "*", "s"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     "s"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.447647040163802*^9, 3.447647256110243*^9}, {
   3.447648081879654*^9, 3.447648084147584*^9}, 3.447652829131774*^9, 
   3.447900350737405*^9, {3.447900534202985*^9, 3.447900590914753*^9}, {
   3.4479274918419027`*^9, 
   3.447927492108796*^9}},ExpressionUUID->"ebe0c180-a546-4708-a2b6-\
c57e4ac7d784"],

Cell[BoxData[
 RowBox[{
  RowBox[{"facProcedural2", "[", "n_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"s", "=", "1"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"s", "=", 
        RowBox[{"i", "*", "s"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "s"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.4479005796125517`*^9, 3.4479005845301933`*^9}, {
  3.4479640825145273`*^9, 
  3.447964082850871*^9}},ExpressionUUID->"8601f896-b47a-4489-98a5-\
