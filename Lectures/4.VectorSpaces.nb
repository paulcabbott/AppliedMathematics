Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "a5b40f0e-75df-471a-99c3-0df6521d8fec"],
 " is an ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[Times]", "n"}], TraditionalForm]],ExpressionUUID->
  "682a3058-9533-45e9-af69-8a38a6d46877"],
 " matrix and there exists ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "3048ffe8-6214-4566-bdc6-a772fea75a12"],
 " linearly independent eigenvectors, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["u",
     FontWeight->"Bold"], "i"], TraditionalForm]],ExpressionUUID->
  "1c5b4ec6-470d-46d2-ba88-fe1e089e9b91"],
 ", these eigenvectors can be used to ",
 StyleBox["diagonalize",
  FontSlant->"Italic"],
 " the matrix. \nLet ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]],ExpressionUUID->
  "f4090e29-f8ad-4b67-bdc7-3f962438d24d"],
 " be the matrix of eigenvectors, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox[
       StyleBox["u",
        FontWeight->"Bold"], "1"], "\[VerticalSeparator]", 
      RowBox[{
       SubscriptBox[
        StyleBox["u",
         FontWeight->"Bold"], "2"], "|", "\[Ellipsis]", "|", 
       SubscriptBox[
        StyleBox["u",
         FontWeight->"Bold"], "n"]}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"88c54e44-9f34-43e6-9802-81a27ec3a614"],
 ", and ",
 Cell[BoxData[
  FormBox["D", TraditionalForm]],ExpressionUUID->
  "f79d4970-7a53-4e5e-94ff-d4657a8e5cb5"],
 " the diagonal matrix with the eigenvalues ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "1"], TraditionalForm]],ExpressionUUID->
  "63af4060-2520-4944-8ded-709e57107c70"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "2"], TraditionalForm]],ExpressionUUID->
  "5e4ca173-9169-4783-ab5c-ee894c3453d2"],
 ", ",
 Cell[BoxData[
  FormBox["\[Ellipsis]", TraditionalForm]],ExpressionUUID->
  "9e554f0d-7e64-4615-b119-776bdc735768"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "n"], TraditionalForm]],ExpressionUUID->
  "641d92a5-5697-4f50-8d7e-c2b5093414ed"],
 " along the diagonal then, since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", ".", " ", 
     SubscriptBox[
      StyleBox["u",
       FontWeight->"Bold"], "i"]}], "\[LongEqual]", 
    RowBox[{
     SubscriptBox["\[Lambda]", "i"], " ", 
     SubscriptBox[
      StyleBox["u",
       FontWeight->"Bold"], "i"]}], "\[LongEqual]", 
    RowBox[{
     SubscriptBox[
      StyleBox["u",
       FontWeight->"Bold"], "i"], 
     StyleBox[" ",
      FontWeight->"Bold"], 
     SubscriptBox["\[Lambda]", "i"]}]}], TraditionalForm]],ExpressionUUID->
  "f0d210f1-5ca5-46e0-92d3-129ea3a88aea"],
 ","
}], "Text",
