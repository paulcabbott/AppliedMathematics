         {
          RowBox[{
           SubscriptBox["b", "n"], "=", 
           RowBox[{
            FractionBox["1", "\[Pi]"], 
            RowBox[{
             SubsuperscriptBox["\[Integral]", 
              RowBox[{"-", "\[Pi]"}], "\[Pi]"], 
             RowBox[{
              RowBox[{"sin", "(", 
               RowBox[{"n", "\[VeryThinSpace]", "x"}], ")"}], 
