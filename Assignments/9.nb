  3.516357314639122*^9}, {3.516357349427384*^9, 3.516357364727283*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ModePlot", "[", 
   RowBox[{"n_", ",", "i_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[CapitalLambda]", ",", "P"}], "}"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[CapitalLambda]", ",", "P"}], "}"}], "=", 
      RowBox[{"Transpose", "[", 
       RowBox[{"Sort", "[", 
        RowBox[{"Transpose", "[", 
         RowBox[{"Eigensystem", "[", 
          RowBox[{"N", "[", 
           SubscriptBox["\[ScriptCapitalM]", "n"], "]"}], "]"}], "]"}], "]"}],
        "]"}]}], ";", 
     RowBox[{"ListPlot", "[", 
      RowBox[{
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "0", "}"}], ",", 
         RowBox[{"P", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
         ",", 
         RowBox[{"{", "0", "}"}]}], "]"}], ",", 
       RowBox[{"Joined", "\[Rule]", "True"}], ",", 
       RowBox[{"PlotLabel", "\[Rule]", 
        RowBox[{"\"\<\[CapitalOmega]=\>\"", "<>", 
         RowBox[{"ToString", "[", 
          SqrtBox[
           RowBox[{
           "\[CapitalLambda]", "\[LeftDoubleBracket]", "i", 
            "\[RightDoubleBracket]"}]], "]"}]}]}], ",", 
       RowBox[{"Mesh", "\[Rule]", "All"}], ",", 
       RowBox[{"DataRange", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"n", "+", "1"}]}], "}"}]}], ",", 
       RowBox[{"PlotRange", "\[Rule]", 
        RowBox[{
         FractionBox["3", 
          RowBox[{"2", " ", 
           SqrtBox["n"]}]], " ", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], ",", "1"}], "}"}]}]}]}], "]"}]}]}], 
